{
  "name": "solid-rollup-boilerplate",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "coverage": "jest --coverage",
    "lint": "eslint --ext .ts,.tsx src/",
    "open:coverage": "npm run coverage && open-cli ./coverage/lcov-report/index.html",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "classcat": "^4.1.0",
    "lodash.throttle": "^4.1.1",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "solid-js": "^0.18.14",
    "solid-typefu-router5": "^0.10.3",
    "workbox-sw": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/dom": "^7.22.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/gapi": "0.0.39",
    "@types/jest": "^26.0.10",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.6.0",
    "@types/workbox-sw": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-solid": "^0.18.14",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dom-expressions": "^0.19.10",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-dom": "^3.1.7",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.7.0",
    "jest": "^26.4.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "postcss-url": "^8.0.0",
    "prettier": "^2.1.0",
    "rollup": "^2.26.5",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-html2": "file:misc/rollup-plugin-html2",
    "rollup-plugin-imagemin": "^0.4.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-manifest-json": "^1.1.1",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-svgo": "^1.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-visualizer": "^4.1.0",
    "rollup-plugin-workbox": "^5.2.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.2.0",
    "typescript": "^4.1.0-dev.20200825",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "testMatch": [
      "<rootDir>/tests/**/*.ts?(x)"
    ],
    "roots": [
      "./"
    ],
    "preset": "ts-jest",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|svg)$": "<rootDir>/tests/mocks/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/tests/mocks/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "assets",
      ".css.d.ts"
    ],
    "setupFilesAfterEnv": [
      "./config/jest.setup.ts"
    ]
  }
}
